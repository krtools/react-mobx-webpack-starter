{
  "name": "react-mobx-webpack2-starter",
  "private": true,
  "version": "0.1.0",
  "description": "Starter Webpack App",
  "main": "",
  "scripts": {
    "start": "node server.js --env=dev",
    "start:local": "node server.js --env=local --buildenv=dev",
    "start:prod": "node server.js --env=prod --buildenv=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve:dist": "node server.js",
    "dist:local": "npm run copy & webpack --env.env=local --env.buildenv=dist",
    "dist:dev": "npm run copy & webpack --env.env=dev --env.buildenv=dist",
    "dist:prod": "npm run copy & webpack --env.env=prod --env.buildenv=dist",
    "lint": "eslint ./src",
    "copy": "npm run clean & copyfiles -f ./src/favicon.ico ./dist",
    "clean": "rimraf \"dist/*\""
  },
  "repository": "",
  "keywords": [],
  "author": "Kyle Rector",
  "devDependencies": {
